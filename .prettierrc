{
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": true,
    "semi": false,
    "singleQuote": true,
    "importOrder": [
        "THIRD_PARTY_MODULES",
        "^@/componets/(.*)$",
        "^@/screens/(.*)$",
        "^@/ui/(.*)$",
        "^@/hooks/(.*)$",
        "^@/assets/(.*)$",
        "^@/shared/(.*)$",
        "^@/utils/(.*)$",
        "^@/services/(.*)$",
        "^@/config/(.*)$",
        "^@/store/(.*)$",
        "^../(.*)",
        "^./(.*)",
        "(.scss)$"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}